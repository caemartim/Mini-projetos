#include <stdio.h>
#include <stdlib.h>

int main () {
	float a, b, c;
	c=0;
	printf("digite um numero para a letra a:");
	scanf("%f", &a);
	printf("digite um numero para a letra b:");
	scanf("%f", &b);
	c = a;
	a = b;
	b = c;
	
	printf("a letra a tem o valor de: %f",a);
	printf ("a letra b tem o valor de: %f", b);
	
	
}
